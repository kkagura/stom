  "main": "index.js",
  "scripts": {
    "dev": "pnpm -F @stom/ui build-comp && run-p dev:shared dev:geo dev:core dev:ui dev:play",
    "dev:play": "cd ./packages/playground && npm run dev",
    "dev:ui": "cd ./packages/ui && npm run dev",
    "dev:geo": "pnpm -F @stom/geo dev",
    "dev:core": "pnpm -F @stom/core dev",
    "dev:shared": "pnpm -F @stom/shared dev",
    "build": "pnpm -F @stom/ui build-comp && pnpm -r exec pnpm run build",
    "build:play": "cd ./packages/playground && npm run build",
    "build:ui": "cd ./packages/ui && npm run build",
    "build:geo": "pnpm -F @stom/geo build",
    "build:core": "pnpm -F @stom/core build",
    "build:shared": "pnpm -F @stom/shared dev",
    "build:dts": "node scripts/types-definitions.js",
    "ci": "pnpm install --frozen-lockfile"
  },
