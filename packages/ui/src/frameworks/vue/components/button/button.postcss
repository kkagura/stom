.st-button {
  font-size: 12px;
  padding: 0px 10px;
  color: #606266;
  background-color: #fff;
  border: 1px solid #dcdfe6;
  border-radius: 2px;
  transition: all 0.3s ease;
  line-height: 22px;
  font-size: 12px;
  box-sizing: border-box;
  cursor: pointer;
  &:hover {
    color: var(--st-color-primary);
    border-color: var(--st-color-primary);
  }
}
